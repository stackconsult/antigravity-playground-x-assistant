{
  "name": "antigravity-playground",
  "version": "1.0.0",
  "description": "Autonomous Agent Engineering Framework",
  "main": "src/index.js",
  "scripts": {
    "test": "node tests/setup.test.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "echo 'Building production kernel v2.3...'",
    "start": "npx -y electron src/electron/main.js",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "ai",
    "agent",
    "autonomous",
    "framework"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "googleapis": "^144.0.0",
    "keytar": "^7.9.0"
  },
  "build": {
    "appId": "com.antigravity.assistant",
    "productName": "Antigravity Assistant",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ],
      "icon": "src/assets/icon.png",
      "identity": null
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/icon.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "src/assets/icon.png"
    }
  }
}